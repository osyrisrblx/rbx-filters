{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"downlevelIteration": true,
		"module": "commonjs",
		"noLib": true,
		"outDir": "out",
		"rootDir": "src",
		"strict": true,
		"target": "es6",
		"types": ["rbx-types"],
		"declaration": true
	},
	"typeAcquisition": {
		"enable": true
	}
}
